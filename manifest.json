{
    "name": "Skyward GPA Calculator - And More",
    "description": "This is an extension which injects various scripts into Skyward to calculate GPA and perform other tasks.",
    "version": "0.4",
    "permissions": [
        "http://*/scripts/wsisa.dll/WService=wsEAplus/*",
		"https://*/scripts/wsisa.dll/WService=wsEAplus/*",
        "tabs",
	    "storage"
    ],
	"icons": {                   
		"16": "icon16.png",
		"38": "icon38.png",
		"48": "icon48.png",
		"128": "icon128.png"  
	},    
	"browser_action": {
        "default_title": "Reee",
        "default_popup": "popup.html",
		"default_icon": {                   
			"16": "icon16.png",
			"38": "icon38.png",
			"48": "icon48.png",
			"128": "icon128.png"  
		}
	},
    "web_accessible_resources": [
        "css/variables.css",
        "css/qsfmain001.css",
        "css/sfhome001.css",
        "css/jquery.cluetip.css",
        "css/sfgradebook001.css",
        "css/qclssbase001.css",
        "css/qclsslogin001.css",
        "sprite/arrows_sprite.png",
        "sprite/printer1.png",
        "sprite/sf_bg_sprite001.png",
        "sprite/sf_icons_sprite.png"
    ],
    "content_scripts": [
        {
        "matches": [
            "http://*/scripts/wsisa.dll/WService=wsEAplus/*",
			"https://*/scripts/wsisa.dll/WService=wsEAplus/*"
        ],
        "js": ["inject.js"],
        "run_at": "document_end"
        }
    ],
    "manifest_version": 2
}
